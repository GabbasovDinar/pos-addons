<odoo>
    <template id="assets_backend" name="cash_box assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/pos_cashbox/static/src/js/cashbox.js"></script>
        </xpath>
    </template>

    <template id="qunit_suite" name="pos_cashbox tests" inherit_id="web.qunit_suite">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/pos_cashbox/static/tests/cashbox_test.js"></script>
        </xpath>
    </template>
</odoo>
